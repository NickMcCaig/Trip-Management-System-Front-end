@page "/MyTrips"
@using BackEndAPI
<PageTitle>My Trips</PageTitle>

<h1>Hello, User!</h1>
Here are your events
<div>
    @foreach (var e in events ?? Enumerable.Empty<EventDto>())
    {
        <h1>@e.Title</h1>
        <h2>@e.LocationDesc</h2>
        <h3>@e.Id</h3>
        <h3>@e.StartDateTime @e.EndDateTime</h3>
        <button class="btn btn-success" @onclick="() => NavigatetoEvent(e.Id)">More info</button>

    }
</div>
<div>
    <p>@ErrorMessage</p>
</div>
